#!/usr/bin/env bash

# Include files
SRC="${BASH_SOURCE%/*}"
if [ ! -d "$SRC" ]; then SRC="$PWD"; fi

. $SRC/cmd
. $SRC/format
. $SRC/functions
. $SRC/update_kernel


REQ_FILE=requirements/req.yml
UBUNTU_KERNEL_URL=kernel.ubuntu.com/~kernel-ppa/mainline/
TEMP_DIRECTRORY=$(mktemp -d)
EMPTY_ARRAY=()
VERSION="0.1.x"
DESCRIPTION="ROCm Machine Learning Stack installer"
TOTALSTEPS=12
BEQUIET=false

